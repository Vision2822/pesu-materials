<script>
function loginCheck(e) {
	var k;
	document.all ? k = e.keyCode : k = e.which;
	return ((k > 63 && k < 91) || (k > 96 && k < 123) || k == 95 || k == 45 || k == 46 || (k >= 48 && k <= 57));
}
